<?xml version="1.0" encoding="UTF-8"?>

<pageflow-definition name="selectSchool">

  <start-state name="start">
    <transition to="selectSchool">
        <action expression="#{schoolSearch.schoolSearch()}" />
    </transition>
  </start-state>

  
    
  <page name="selectSchool" view-id="/schoolSearch.xhtml">
    <redirect />
    <transition name="next" to="home">
        <action expression="#{schoolSearch.setSchool(school)}" />
        <action expression="#{schoolSearch.end()}" />
        
    </transition>
    
  </page>
  
  <page name="home" view-id="/home.xhtml">
    <redirect />
    <end-conversation/>
  </page>
  
</pageflow-definition>
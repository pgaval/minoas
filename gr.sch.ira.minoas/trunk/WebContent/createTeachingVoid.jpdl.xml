<?xml version="1.0" encoding="UTF-8"?>

<pageflow-definition
  name="createTeachingVoid">

	<start-state name="start">
		
	</start-state>
	<start-page name="start" view-id="/createTeachingVoid.xhtml">
		<redirect/>
		<transition name="evaluateSchoolNeeded" to="schoolSelected" />
	</start-page>
	
	<decision name="schoolSelected" expression="#{true}">
        <transition name="true" to="enterVoidDetails" />
        <transition name="false" to="selectSchool" />
    </decision>
    <page name="selectSchool" view-id="/schoolSearch.xhtml"></page>
	
</pageflow-definition>
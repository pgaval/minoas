<?xml version="1.0" encoding="UTF-8"?>

<pageflow-definition xmlns="http://jboss.com/products/seam/pageflow"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://jboss.com/products/seam/pageflow http://jboss.com/products/seam/pageflow-2.0.xsd" name="school-record">

  <start-page name="startPage" view-id="/schools.xhtml">
  		<redirect/>
       <transition to="schoolPage" name="school-selected" />
  </start-page>
  
  <page view-id="/school.xhtml" name="schoolPage">
  	<redirect/>
    <transition to="startPage" name="selectSchool" />
    <transition to="home" name="end" />
  </page>
  
  <page view-id="/home.xhtml" name="home">
    <end-conversation/>
    <redirect />
  </page>
  
  
  
</pageflow-definition>